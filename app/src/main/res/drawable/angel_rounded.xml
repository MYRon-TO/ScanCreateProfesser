<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle"> <!-- 注意：这里的"="后面应该是具体的值，而不是一个数组 -->


    <!-- 设置背景颜色 -->
    <solid android:color="#FFF"/> <!-- 红色背景 -->

    <!-- 设置边框宽度、颜色和样式 -->
    <stroke
        android:width="2dp"
        android:color="#FFF" />

    <!-- 设置圆角 -->
    <corners android:radius="10dp"/>
</shape>